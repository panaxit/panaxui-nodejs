{
  "name": "panaxui-nodejs",
  "version": "0.0.1",
  "author": "Benjamin Orozco <benoror@gmail.com>",
  "description": "PanaxUI API Backend - Node.js",
  "main": "app.js",
  "private": true,
  "license": "Copyright 2015 PanaxIT (R)",
  "scripts": {
    "start": "node node_modules/nodemon/bin/nodemon.js -i cache ./bin/www",
    "start:forever": "node node_modules/forever/bin/forever start ./bin/www",
    "restart:forever": "node node_modules/forever/bin/forever restartall",
    "stop:forever": "node node_modules/forever/bin/forever stopall",
    "test:all": "node node_modules/jasmine-node/bin/jasmine-node spec",
    "test": "npm run test:all"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:all"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://benoror@bitbucket.org/benoror/panaxui-nodejs.git"
  },
  "dependencies": {
    "body-parser": "~1.10.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.1.1",
    "express": "~4.11.0",
    "express-session": "^1.10.1",
    "frisby": "^0.8.5",
    "jade": "~1.9.0",
    "libxslt": "^0.4.1",
    "mkdirp": "^0.5.0",
    "morgan": "~1.5.1",
    "node-pate": "0.0.6",
    "panaxjs": "git+https://benoror@bitbucket.org/panaxit/panaxjs.git",
    "serve-favicon": "~2.2.0"
  },
  "devDependencies": {
    "forever": "^0.14.1",
    "ghooks": "^0.3.2",
    "jasmine-node": "^1.14.5",
    "nodemon": "^1.3.7"
  }
}
